    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^8.2.2",
    "c8": "^7.7.2",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "mocha": "^8.4.0",
    "react-native-ble-plx": "^2.0.2",
    "testdouble": "^3.16.1",
    "typescript": "^4.3.2"
  }
